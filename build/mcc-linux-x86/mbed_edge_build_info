#ifndef __MBED_EDGE_BUILD_BUILD_INFO_H__
#define __MBED_EDGE_BUILD_BUILD_INFO_H__
//  build info, #include MBED_EDGE_BUILD_BUILD_INFO_HEADER to access
#define MBED_EDGE_BUILD_BUILD_YEAR 2018 // UTC year
#define MBED_EDGE_BUILD_BUILD_MONTH 4 // UTC month 1-12
#define MBED_EDGE_BUILD_BUILD_DAY 4 // UTC day 1-31
#define MBED_EDGE_BUILD_BUILD_HOUR 7 // UTC hour 0-24
#define MBED_EDGE_BUILD_BUILD_MINUTE 41 // UTC minute 0-59
#define MBED_EDGE_BUILD_BUILD_SECOND 13 // UTC second 0-61
#define MBED_EDGE_BUILD_BUILD_UUID 951e74fa-7bed-4ba9-b957-714a83b96c05 // unique random UUID for each build
#define MBED_EDGE_BUILD_BUILD_VCS_ID 057985fadacda0717ed2049141f304e9abab3bfe // git or mercurial hash
#define MBED_EDGE_BUILD_BUILD_VCS_CLEAN 1 // evaluates true if the version control system was clean, otherwise false
#define MBED_EDGE_BUILD_BUILD_VCS_DESCRIPTION R0.4.4-RC1 // git describe or mercurial equivalent
#endif // ndef __MBED_EDGE_BUILD_BUILD_INFO_H__
